piVersion: kubeadm.k8s.io/v1alpha1
kind: MasterConfiguration
kubeProxy:
  config:
    mode: ipvs
networking:
  serviceSubnet: {{ kube_cluster_cidr }}
  podSubnet: {{ kube_pod_cidr }}
apiServerExtraArgs:
